{
  "name": "ts-api",
  "version": "1.0.0",
  "description": "Serverless Cloud TypeScript API template",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "start": "cloud",
    "test": "cloud test",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint --fix .",
    "pretty": "prettier --write .",
    "prepare": "husky install",
    "precommit": "npm run lint-fix && npm run pretty",
    "prepush": "npm run lint"
  },
  "devDependencies": {
    "@serverless/cloud": "^2.10.13",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1"
  },
  "serverless": {
    "app": "pb-bot",
    "org": "dnhess20"
  },
  "dependencies": {
    "@discordjs/rest": "^1.1.0",
    "discord-api-types": "^0.37.10",
    "discord-interactions": "^3.2.0",
    "luxon": "^3.0.4"
  }
}
